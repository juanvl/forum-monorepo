{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "setupFilesAfterEnv": [
    "<rootDir>/test/setup-e2e.ts"
  ],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "modulePathIgnorePatterns": [
    "<rootDir>/dist",
    "<rootDir>/generated/prisma/dist",
    "<rootDir>/node_modules/.pnpm"
  ],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^test/(.*)$": "<rootDir>/test/$1",
    "^generated/prisma/client$": "<rootDir>/generated/prisma/client",
    "^@/(.*)$": "<rootDir>/../../packages/domain/src/$1"
  }
}